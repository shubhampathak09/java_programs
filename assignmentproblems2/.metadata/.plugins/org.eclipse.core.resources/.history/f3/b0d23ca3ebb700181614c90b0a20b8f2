import java.util.Scanner;

public class Main {
	
	
	public static void main(String[] args)
	{
		MemberVariable MemberVariableObject=new MemberVariable();
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the name of your gf here");
	    String name=s.nextLine();
	    MemberVariableObject.setname(name);
	    MemberVariableObject.display();
	}

}
